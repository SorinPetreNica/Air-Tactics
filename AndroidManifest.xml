		<!-- The application's publisher ID assigned by AdMob -->
		<meta-data
			android:value="a14de914472599e"
			android:name="ADMOB_PUBLISHER_ID" />
		<!-- AdMobActivity definition -->
		<activity
			android:name="com.google.ads.AdActivity"
			android:theme="@android:style/Theme.NoTitleBar.Fullscreen"
			android:configChanges="orientation|keyboard|keyboardHidden" />
		<activity
			android:name="com.internet.XMPPClient"
		></activity>
		<service
			android:name="com.internet.XMPPService"
		></service>
		<activity
			android:name="com.internet.ui.CreateUser"
		></activity>
		<activity
			android:name="com.internet.ui.GameRoom"
		></activity>
