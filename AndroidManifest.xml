		android:label="@string/app_name"
		android:clearTaskOnLaunch="true"
		android:name="com.airtactics.MainAPP" android:debuggable="false">
		<activity
			android:name="com.airtactics.Air"
			android:label="@string/app_name"
		>
		</activity>
		<activity
			android:name="com.airtactics.PlayScene"
			android:clearTaskOnLaunch="true"
		>
			<intent-filter>
				<action
					android:name="android.intent.action.MAIN" />
				<category
					android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name=".ModeSelectionActivity" />
		<activity
			android:name=".GamePlayActivity" />
		<activity
			android:name=".AchievementsActivity" />
		<activity
			android:name=".AchievementActivity" />
		<!-- uses the following ScoreloopUI activities -->
		<activity
			android:name="com.scoreloop.client.android.ui.EntryScreenActivity"
			android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" />
		<activity
			android:name="com.scoreloop.client.android.ui.BuddiesScreenActivity"
			android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" />
		<activity
			android:name="com.scoreloop.client.android.ui.LeaderboardsScreenActivity"
			android:theme="@android:style/Theme.Black.NoTitleBar.Fullscreen" />
